<Window x:Class="AnalyStock.GrapImage.PricePanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"       
        mc:Ignorable="d"
        Title="PricePanel" Height="95" Width="85" ShowInTaskbar="False"  WindowStyle="None" AllowsTransparency="True" Topmost="True" Background="{x:Null}" >
    <Border CornerRadius="5" Background="#FFFFC3B2" Opacity="0.7">       
        <StackPanel >
            <Canvas x:Name="DrawPricePanel" />
        </StackPanel>
    </Border>
</Window>
